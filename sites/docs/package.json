{
	"name": "mode-watcher-docs",
	"version": "0.0.3",
	"private": true,
	"description": "Documentation site for mode-watcher",
	"scripts": {
		"dev": "concurrently \"pnpm:dev:content\" \"pnpm:dev:svelte\"",
		"dev:content": "contentlayer dev",
		"dev:svelte": "vite dev",
		"build": "contentlayer build && vite build",
		"build:content": "contentlayer build",
		"build:content-cachebust": "contentlayer build --clearCache",
		"preview": "vite preview",
		"check": "pnpm build:content && svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch"
	},
	"license": "MIT",
	"author": {
		"name": "Huntabyte",
		"url": "https://x.com/huntabyte"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/svecosystem/mode-watcher.git",
		"directory": "sites/docs"
	},
	"devDependencies": {
		"@sveltejs/adapter-cloudflare": "^4.0.0",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.0",
		"@tailwindcss/typography": "^0.5.10",
		"@types/eslint": "8.56.0",
		"@types/hast": "^3.0.4",
		"@types/mdast": "^4.0.3",
		"@types/node": "^20.14.10",
		"autoprefixer": "^10.4.16",
		"clsx": "^2.1.0",
		"concurrently": "^8.2.2",
		"contentlayer": "^0.3.4",
		"hast-util-to-html": "^9.0.0",
		"mdsx": "^0.0.5",
		"phosphor-svelte": "^1.4.2",
		"postcss": "^8.4.32",
		"postcss-load-config": "^5.0.2",
		"rehype-pretty-code": "^0.13.0",
		"remark-gfm": "^4.0.0",
		"shiki": "^1.1.1",
		"svelte": "^4.2.7",
		"svelte-check": "^3.6.0",
		"tailwind-merge": "^2.2.0",
		"tailwind-variants": "^0.1.20",
		"tailwindcss": "^3.3.6",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"unified": "^11.0.4",
		"unist-util-visit": "^5.0.0",
		"vite": "^5.0.3",
		"vitest": "^1.2.0"
	},
	"type": "module",
	"dependencies": {
		"@melt-ui/svelte": "^0.70.0",
		"bits-ui": "^0.14.0",
		"mode-watcher": "workspace:*",
		"rehype-slug": "^6.0.0",
		"vaul-svelte": "^0.0.7"
	}
}
